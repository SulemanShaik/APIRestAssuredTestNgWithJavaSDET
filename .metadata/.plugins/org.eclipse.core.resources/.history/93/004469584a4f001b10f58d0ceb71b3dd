package com.qa.PersistentJava;

public class PostandPreOperators {

	public static void main(String[] args) {

		
		int i=100;
		
		// i=i+1  we can use this 
		 // i++   we can use this 
		
		System.out.println(i);  //100
		System.out.println(i++); //100
		System.out.println(i);    //101
		
		
		int j=500 + i++;
		
		System.out.println(j);  //601
		System.out.println(i);  //102
		 
		
		//pre increment
		
	}

}
